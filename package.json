{
  "name": "webpack-react-ant",
  "version": "1.0.0",
  "description": "Webpack for ant design.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "analyzer": "webpack --config webpack.analyzer.js",
    "predev": "npm run clean && webpack --config webpack.dll.js --env=development",
    "prebuild": "npm run clean && webpack --config webpack.dll.js --env=production",
    "dev": "webpack-dev-server --config webpack.dev.js  --env=development",
    "build": "webpack --config webpack.prod.js --env=production",
    "start": "node express.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afeiship/webpack-react-ant.git"
  },
  "keywords": [
    "webpack",
    "react",
    "pc",
    "ant-design",
    "site",
    "website"
  ],
  "author": "afei",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afeiship/webpack-react-ant/issues"
  },
  "homepage": "https://github.com/afeiship/webpack-react-ant#readme",
